<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
      <Version>2.5.0</Version>
      <Authors>Ruben Labruyere</Authors>
      <Company>Ruben Labruyere</Company>
      <PackageId>TiledCS</PackageId>
      <Description>
        TiledCS is a library for loading Tiled maps and tilesets. This version supports Tiled 1.5.0
      </Description>
      <RepositoryUrl>https://github.com/thebonejarmer/tiledcs.git</RepositoryUrl>
      <PackageTags>tiled,tmx,tsx</PackageTags>
      <GenerateDocumentationFile>true</GenerateDocumentationFile>
    </PropertyGroup>

    <PropertyGroup>
      <TargetFramework>netstandard2.0</TargetFramework>
    </PropertyGroup>

</Project>
